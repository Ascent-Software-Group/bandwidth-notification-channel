{
  "name": "ankurk91/bandwidth-notification-channel",
  "description": "Bandwidth SMS notification channel for Laravel php framework.",
  "keywords": [
    "laravel",
    "notifications",
    "bandwidth",
    "sms",
    "mms"
  ],
  "homepage": "https://github.com/ankurk91/bandwidth-notification-channel",
  "license": "MIT",
  "authors": [
    {
      "name": "ankurk91",
      "homepage": "https://ankurk91.github.io",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.2",
    "ext-json": "*",
    "guzzlehttp/guzzle": "^6.3||^7.0",
    "illuminate/log": "^6.0||^7.0",
    "illuminate/notifications": "^6.0||^7.0",
    "illuminate/support": "^6.0||^7.0"
  },
  "require-dev": {
    "mockery/mockery": "^1.3.0",
    "phpunit/phpunit": "^8.4.1||^9.0"
  },
  "autoload": {
    "psr-4": {
      "NotificationChannels\\Bandwidth\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "NotificationChannels\\Bandwidth\\Test\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true,
    "preferred-install": "dist"
  },
  "scripts": {
    "test": "vendor/bin/phpunit --testdox --color=always"
  },
  "extra": {
    "laravel": {
      "providers": [
        "NotificationChannels\\Bandwidth\\BandwidthServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
