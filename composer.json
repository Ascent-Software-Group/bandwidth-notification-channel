{
  "name": "ankurk91/bandwidth-notification-channel",
  "description": "Bandwidth SMS notification channel for Laravel php framework.",
  "keywords": [
    "laravel",
    "notifications",
    "bandwidth",
    "sms",
    "mms"
  ],
  "homepage": "https://github.com/ankurk91/bandwidth-notification-channel",
  "license": "MIT",
  "authors": [
    {
      "name": "ankurk91",
      "homepage": "https://ankurk91.github.io",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.1.3",
    "ext-json": "*",
    "guzzlehttp/guzzle": "^6.3",
    "illuminate/log": "~5.6||^6.0",
    "illuminate/notifications": "~5.6||^6.0",
    "illuminate/support": "~5.6||^6.0"
  },
  "require-dev": {
    "mockery/mockery": "^1.1",
    "phpunit/phpunit": "^7.5.15||^8.3.0"
  },
  "autoload": {
    "psr-4": {
      "NotificationChannels\\Bandwidth\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "NotificationChannels\\Bandwidth\\Test\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true,
    "preferred-install": "dist"
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "NotificationChannels\\Bandwidth\\BandwidthServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
